# SalesLens Environment Variables
# Copy this to .env and fill in your keys

# Presage SmartSpectra API Key
# Get it: https://physiology.presagetech.com/auth/register
PRESAGE_API_KEY=your_presage_api_key_here

# ElevenLabs API Key
# Get it: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Anthropic Claude API Key
# Get it: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database path (relative to project root)
DB_PATH=sync-engine/data/saleslens.db

# API Server
API_HOST=127.0.0.1
API_PORT=8000
